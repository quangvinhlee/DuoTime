{
  "name": "duotime-workspace",
  "version": "1.0.0",
  "description": "DuoTime - A couples' reminder and activity tracking app",
  "private": true,
  "scripts": {
    "build": "nx run-many --target=build",
    "test": "nx run-many --target=test",
    "lint": "nx run-many --target=lint",
    "lint:fix": "nx run-many --target=lint --fix",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "affected:build": "nx affected --target=build",
    "affected:test": "nx affected --target=test",
    "affected:lint": "nx affected --target=lint",
    "graph": "nx graph",
    "reset": "nx reset",
    "clean": "nx reset && rm -rf node_modules && npm install",
    "ci:static-analysis": "npm run lint && npm run format:check && npm run markdown:lint && npm run spell:check",
    "ci:test": "npm run test",
    "ci:build": "npm run build",
    "markdown:lint": "npx markdownlint-cli \"**/*.md\" --ignore \"**/node_modules/**\" --ignore \"dist/**\" --ignore \"build/**\" --ignore \".git/**\"",
    "spell:check": "npx cspell \"**/*.{md,ts,tsx,js,jsx,json}\" --gitignore",
    "spell:fix": "npx cspell \"**/*.{md,ts,tsx,js,jsx,json}\" --fix --gitignore",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:mobile\" --kill-others --prefix-colors \"blue,green\" --prefix \"[{name}]\" --names \"backend,mobile\" --concurrency 2 --raw",
    "dev:backend": "cd apps/backend && npm run start:dev",
    "dev:mobile": "cd apps/mobile && npx expo start --port 8082",
    "start": "nx run-many --target=serve --parallel",
    "db:generate": "cd apps/backend && npx prisma generate",
    "db:migrate": "cd apps/backend && npx prisma migrate dev",
    "db:push": "cd apps/backend && npx prisma db push",
    "db:studio": "cd apps/backend && npx prisma studio --port 5556",
    "prepare": "husky"
  },
